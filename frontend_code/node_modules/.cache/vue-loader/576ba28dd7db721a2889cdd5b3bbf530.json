submenu :index=\"system.id + ''\" v-for=\"system in systemList\" :key=\"system.id\">\n            <template slot=\"title\">\n              <i :class=\"iconObj[system.id]\"></i>\n              <span>{{system.systemName}}</span>\n            </template>\n            <!-- 二级菜单 -->\n            <el-menu-item-group>\n              <el-menu-item\n                :index=\"'/'+subSystem.path\"\n                v-for=\"subSystem in system.children\"\n                :key=\"subSystem.id\"\n              >\n                <i :class=\"subSystem.iconObj\"></i>\n                <span>{{subSystem.systemName}}</span>\n              </el-menu-item>\n            </el-menu-item-group>\n          </el-submenu>\n        </el-menu>\n      </el-aside>\n      <el-main>\n        <router-view></router-view>\n      </el-main>\n    </el-container>\n  </el-container>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      // 左侧系统菜单数据\n      systemList: [\n        //   主系统菜单数据\n        {\n          id: 1,\n          systemName: \"网格管理系统\",\n          // 子系统菜单数据\n          children: [\n            {\n              id: \"1-1\",\n              systemName: \"用户管理\",\n              path: \"usermanage\",\n              iconObj: \"el-icon-user\"\n            }\n          ]\n        },\n        { id: 2, systemName: \"待开发\" }\n      ],\n      // 菜单图标\n      iconObj: {\n        \"1\": \"el-icon-office-building\",\n        \"2\": \"el-icon-lock\",\n        \"3\": \"el-icon-lock\",\n        \"4\": \"el-icon-lock\",\n        \"5\": \"el-icon-lock\"\n      },\n      isCollapse: false,\n      activePath: \"\"\n    };\n  },\n  methods: {\n    returnHome() {\n      this.$router.push(\"./Home\");\n    },\n    logout() {\n      this.$router.push(\"./Login\");\n    }\n  }\n};\n</script>\n\n<style lang=\"less\">\n.home-container {\n  height: 100%;\n}\n\n.home_title {\n  font-size: 24px;\n  cursor: pointer;\n}\n\n.el-header {\n  background-color: #3f4648;\n  color: #fff;\n  //   text-align: cente